#include <iostream>
#include <vector>
#include <algorithm>
#include <string>
using namespace std;

int main() {
  int a, b;
  cin >> a >> b;
  vector <int> v1;
  for(int i = 0; i < a; i++){
    int d;
    cin >> d;
    if(b - d >= 0){
      v1.push_back(d);
    }
  }
  cout << v1.size() / 3;
    return 0;
}
