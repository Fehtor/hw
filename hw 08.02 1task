#include <iostream>
#include <vector>
using namespace std;

int main() {
  int N;
  cin >> N;
  vector <int> v;
  for(int i = 0; i < N; i++){
    int a;
    cin >> a;
    v.push_back(a);
  }
vector<int> v2;
  for(int j = 0; j < N; j++){
    for(int i = 0; i < N; i++){
    if(v[i] == j + 1){
      v2.push_back(N - i);
    }
  }    
}
  for(int i = 0; i < N; i++){
    cout << v2[i] << "\n";
  }
  
    return 0;
}
